<field name="days">
    <argument name="data" xsi:type="array">
        <item name="options" xsi:type="object">Custform\Newmodule\Model\Source\Delivery</item>
        <item name="config" xsi:type="array">
            <item name="dataType" xsi:type="string">text</item>
            <item name="label" xsi:type="string" translate="true">Days To Deliver</item>
            <item name="formElement" xsi:type="string">select</item>
            <item name="source" xsi:type="string">days</item>
            <item name="dataScope" xsi:type="string">days</item>
            <item name="validation" xsi:type="array">
                <item name="required-entry" xsi:type="boolean">true</item>
            </item>
        </item>
    </argument>
</field>


     
      
    </fieldset>
</form>